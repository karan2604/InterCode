<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="750.0" prefWidth="700.0" stylesheets="@application.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">

   <children>
      <StackPane fx:id="Upper_StackPane" prefHeight="50.0" prefWidth="1200.0" />
      <HBox fx:id="HBox_compile" prefHeight="700.0" prefWidth="1200.0">
         <children>
            <VBox fx:id="VBox_Compile" prefHeight="700.0" prefWidth="800.0" spacing="5.0">
               <children>
                  <StackPane alignment="TOP_RIGHT" prefHeight="30.0" prefWidth="800.0">
                     <children>
                        <ComboBox fx:id="Select_lang" onAction="#ComboboxSelectLang" prefHeight="26.0" prefWidth="127.0" promptText="Select lauguage" />
                     </children>
                     <padding>
                        <Insets top="5.0" />
                     </padding></StackPane>
                  <TextArea fx:id="CompileTxtArea" onKeyPressed="#CompileTxtAreaKeyPressed" prefHeight="540.0" prefWidth="800.0" />
                  <HBox alignment="TOP_RIGHT" prefHeight="30.0" prefWidth="800.0" spacing="5.0">
                     <children>
                        <Button fx:id="btncompile" mnemonicParsing="false" onAction="#btncompileaction" text="Compile">
                           <HBox.margin>
                              <Insets right="10.0" />
                           </HBox.margin>
                        </Button>
                        <Button layoutX="508.0" layoutY="10.0" mnemonicParsing="false" text="Run">
                           <HBox.margin>
                              <Insets right="10.0" />
                           </HBox.margin>
                        </Button>
                     </children></HBox>
                  <HBox prefHeight="100.0" prefWidth="200.0" spacing="100.0">
                     <children>
                        <TextArea prefHeight="97.0" prefWidth="350.0" />
                        <TextArea layoutX="10.0" layoutY="10.0" prefHeight="97.0" prefWidth="350.0" />
                     </children>
                  </HBox>
               </children></VBox>
            <VBox fx:id="VBox_Chat" prefHeight="700.0" prefWidth="400.0" />
         </children></HBox>
   </children>
</VBox>
